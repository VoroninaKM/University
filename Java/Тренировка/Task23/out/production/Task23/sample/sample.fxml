<GridPane alignment="center" hgap="10" vgap="10" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.Controller">
   <columnConstraints>
      <ColumnConstraints />
   </columnConstraints>
   <rowConstraints>
      <RowConstraints />
   </rowConstraints>
   <children>
      <AnchorPane prefHeight="542.0" prefWidth="662.0">
         <children>
            <Label fx:id="tr" alignment="CENTER" layoutX="5.0" layoutY="9.0" prefHeight="408.0" prefWidth="650.0" style="-fx-background-color: #FFFFF0;" textAlignment="CENTER" wrapText="true" />
            <Button fx:id="add" layoutX="416.0" layoutY="472.0" mnemonicParsing="false" onAction="#add" prefHeight="55.0" prefWidth="231.0" text="Add new node" />
            <Button fx:id="delete" layoutX="14.0" layoutY="472.0" mnemonicParsing="false" onAction="#delete" prefHeight="55.0" prefWidth="231.0" text="Delete node" />
            <TextField fx:id="nodeValue" layoutX="282.0" layoutY="476.0" prefHeight="47.0" prefWidth="95.0" />
            <Label layoutX="316.0" layoutY="437.0" />
            <Label layoutX="296.0" layoutY="441.0" prefHeight="26.0" prefWidth="68.0" text="Node value" textAlignment="JUSTIFY" />
         </children>
      </AnchorPane>
   </children>
</GridPane>
